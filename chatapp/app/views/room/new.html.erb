<h1>ROOM</h1>
<div class="room_button">
  <div class="back_button">
    <%= link_to "戻る", "/room" %>
  </div>
</div>

<h1>新規作成</h1>
<%= form_with url: room_create_path, model: @room, local: true do |f| %>
  <%= f.label :name %>
  <%= f.text_field :name %>
  <%= f.label :describe %>
  <%= f.text_field :describe %>
  <%= f.label :image %>
  <%= f.file_field :image %>
  <%= f.submit "登録する" %>
<% end %>
