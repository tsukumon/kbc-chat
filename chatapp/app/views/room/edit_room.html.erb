<%= form_with url: update_room_path(@room.id), model: @room, local: true do |f| %>
  <%= render "error_messages", model: @room %>
  <%= f.label :category %>
  <%= f.text_field :category %>
  <%= f.label :name %>
  <%= f.text_field :name %>
  <%= f.label :describe %>
  <%= f.text_area :describe %>
  <%= f.label :image %>
  <%= f.file_field :image %>
  <%= f.submit "編集する" %>
<% end %>